.data 
.text
.include "macros.asm"
.globl strncpy #(a1- buf 1, a2 - buf2, a3- len)
strncpy:
	strlen(a1)
	mv t1 a0
	strlen(a2)
	mv t2 a0
	li t3 1
	for:
		beq t3 a3 end_for
		bge t3 t2 fill
		lb t4 (a2)
		sb t4 (a1)
		j next
		fill:
			sb t0 (a1)
		next:
			addi a1 a1 1
			addi a2 a2 1
			addi t3 t3 1
			b for
		end_for:
				
	ret
